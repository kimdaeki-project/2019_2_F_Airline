<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.airline.a1.limo.LimoMapper">
  	<select id="limoList" parameterType="com.airline.a1.limo.LimoVO" resultType="com.airline.a1.limo.LimoVO">
  		select * from limo
  	</select>
  	
  	<insert id="limoBook" parameterType="com.airline.a1.limo.LimoVO">
  		insert into limo values(0, curdate(), #{limoPrice}, #{id}, #{depLoc}, #{arrLoc})
  	</insert>
  </mapper>
